<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='resources'
		type='org.qualipso.factory.ui.service.evaluation.client.resources.EvaluationResources' />
	<g:FlowPanel>
		<g:FlowPanel styleName='{resources.style.bodybox}' ui:field="mainPanel">
			<g:Label ui:field="projectNameLabel" styleName='{resources.style.label}'>Project name</g:Label>
			<g:TextBox ui:field="projectNameTextBox" styleName="{resources.style.textbox}"></g:TextBox>
			<g:Label ui:field="projectVersionLabel" styleName='{resources.style.label}'>Project
				version</g:Label>
			<g:TextBox ui:field="projectVersionTextBox" styleName="{resources.style.textbox}"></g:TextBox>
			<g:Label ui:field="releaseLabel" styleName='{resources.style.label}'>Release</g:Label>
			<g:TextBox ui:field="releaseTextBox" styleName="{resources.style.textbox}"></g:TextBox>
			<g:Label ui:field="revisionLabel" styleName='{resources.style.label}'>Revision</g:Label>
			<g:TextBox ui:field="revisionTextBox" styleName="{resources.style.textbox}"></g:TextBox>
			<g:Label ui:field="repositoryTypeLabel" styleName='{resources.style.label}'>SVN path</g:Label>
			<g:ListBox ui:field="repositoryTypeListBox" styleName="{resources.style.listbox}"></g:ListBox>
			<g:Label ui:field="repositoryPathLabel" styleName='{resources.style.label}'>Repository
				path</g:Label>
			<g:TextBox ui:field="repositoryPathTextBox" styleName="{resources.style.textbox}"></g:TextBox>
			<g:Label ui:field="repositoryUsernameLabel" styleName='{resources.style.label}'>Repository
				username</g:Label>
			<g:TextBox ui:field="repositoryUsernameTextBox" styleName="{resources.style.textbox}"></g:TextBox>
			<g:Label ui:field="repositoryPasswordLabel" styleName='{resources.style.label}'>Repository
				password</g:Label>
			<g:TextBox ui:field="repositoryPasswordTextBox" styleName="{resources.style.textbox}"></g:TextBox>

			<g:Label ui:field="analysisTimeframeBeginLabel" styleName='{resources.style.label}'>Analysis
				timeframe begin</g:Label>
			<g:TextBox ui:field="analysisTimeframeBeginTextBox" styleName="{resources.style.textbox}"></g:TextBox>
			<g:Label ui:field="analysisTimeframeEndLabel" styleName='{resources.style.label}'>Analysis
				timeframe end</g:Label>
			<g:TextBox ui:field="analysisTimeframeEndTextBox" styleName="{resources.style.textbox}"></g:TextBox>

			<g:Label ui:field="minorReleasePatternLabel" styleName='{resources.style.label}'>Minor
				release pattern</g:Label>
			<g:TextBox ui:field="minorReleasePatternTextBox" styleName="{resources.style.textbox}"></g:TextBox>
			<g:Label ui:field="majorReleasePatternLabel" styleName='{resources.style.label}'>Major
				release pattern</g:Label>
			<g:TextBox ui:field="majorReleasePatternTextBox" styleName="{resources.style.textbox}"></g:TextBox>

			<g:Label ui:field="cvsConnectionProtocolLabel" styleName='{resources.style.label}'>CVS
				connection protocol</g:Label>
			<g:ListBox ui:field="cvsConnectionProtocolListBox" styleName="{resources.style.listbox}"></g:ListBox>
			<g:Label ui:field="buildTypeLabel" styleName='{resources.style.label}'>Build type</g:Label>
			<g:ListBox ui:field="buildTypeListBox" styleName="{resources.style.listbox}"></g:ListBox>

			<g:Label ui:field="testReportPathLabel" styleName='{resources.style.label}'>Test report
				path</g:Label>
			<g:TextBox ui:field="testReportPathTextBox" styleName="{resources.style.textbox}"></g:TextBox>
			<g:Label ui:field="jupiterReviewPathLabel" styleName='{resources.style.label}'>Jupiter
				review path</g:Label>
			<g:TextBox ui:field="jupiterReviewPathTextBox" styleName="{resources.style.textbox}"></g:TextBox>
		</g:FlowPanel>
		<g:FlowPanel ui:field="buttonPanel">
			<g:Button ui:field="submitButton"
				styleName='{resources.style.button} {resources.style.buttonforward}'>Submit</g:Button>
			<g:Button ui:field="cancelButton"
				styleName='{resources.style.button} {resources.style.buttonforward}'>Cancel</g:Button>
		</g:FlowPanel>
		<g:FlowPanel ui:field="gqmResultPanel">
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 