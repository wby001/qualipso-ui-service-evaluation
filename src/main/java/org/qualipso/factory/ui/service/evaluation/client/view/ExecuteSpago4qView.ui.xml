<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='resources'
		type='org.qualipso.factory.ui.service.evaluation.client.resources.EvaluationResources' />
	<g:FlowPanel>
		<g:FlowPanel>
			<g:Label ui:field="chooseDocumentLabel" styleName='{resources.style.label}'>Choose document</g:Label>
			<g:ListBox ui:field="chooseDocumentListBox" styleName="{resources.style.listbox}"></g:ListBox>
			<g:Button ui:field="goOnButton" styleName='{resources.style.button} {resources.style.buttonforward}'>Go on</g:Button>
		</g:FlowPanel>
		<g:FlowPanel ui:field="resultPanel">
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 